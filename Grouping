-- Question 1
SELECT
    MAX(MaxTemperatureF)
    zip
FROM
    weather
    
-- Question 2 
SELECT
	start_station,
    Count(start_station)
FROM
    trips
GROUP by start_station

--Question 3
SELECT
    min(duration)
FROM
    trips

--Question 4
SELECT
	end_station,
    AVG(duration)
FROM
    trips
GROUP by end_station
 
